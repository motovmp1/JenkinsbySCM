node('master'){
    stage('Checkout master'){
        echo 'git'
	git credentialsId: 'motovmp1', url: 'https://github.com/motovmp1/JenkinsbySCM.git'
	bat 'ping 8.8.8.8 -n 10 > nul'
    }
    stage ('Build'){
        echo 'build'
	bat 'ping 8.8.8.8 -n 10 > nul'
    }
    stage  ('Send Email status'){
    	emailext body: 'Teste APP Pass', subject: 'Teste Jenkins', to: 'vinicius.pinho@bee-eng.pt'
    }
    stage ('cleaner'){
    	echo 'cleaner stage'
    }
    
}
